{
  "name": "sarigama",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://lucidity.io",
  "repository": "git@github.com:luciditeam/lucidity.git",
  "scripts": {
    "prebuild": "rm -rf output/dist",
    "build": "npm run build:vendor && npm run build:prod",
    "build:debug": "tsc -p ./config/tsconfig.debug.json --pretty",
    "build:prod": "webpack --config webpack/prod.js",
    "build:vendor": "webpack --config webpack/vendor.js",
    "anaylyse:app": "webpack --config webpack.config.js --json | webpack-bundle-size-analyzer",
    "anaylyse:vendor": "webpack --config vendor.webpack.config.js --json | webpack-bundle-size-analyzer",
    "deploy:github": "gh-pages -d output/dist",
    "deploy": "firebase deploy",
    "predeploy": "npm run build",
    "postinstall": "npm run build:vendor",
    "start": "webpack-dev-server --inline --content-base output/dist --config webpack/dev.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "@cerebral/firebase": "next",
    "@cerebral/react": "next",
    "@cerebral/router": "next",
    "@types/codemirror": "0.0.50",
    "@types/katex": "^0.5.0",
    "base64-arraybuffer": "^0.1.5",
    "bulma": "^0.2.3",
    "cerebral": "next",
    "clipboard-js": "^0.3.3",
    "codemirror": "^5.31.0",
    "firebase-functions": "^0.7.1",
    "font-awesome": "^4.7.0",
    "function-tree": "next",
    "js-yaml": "^3.10.0",
    "katex": "^0.9.0-alpha1",
    "marked": "^0.3.6",
    "outdent": "^0.3.0",
    "polished": "^1.7.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-styled-flexboxgrid": "^2.0.3",
    "seedrandom": "^2.4.2",
    "string-hash": "^1.1.3",
    "styled-components": "^2.1.2",
    "tone": "^0.9.0",
    "uuid": "^3.0.1",
    "vexflow": "^1.2.83"
  },
  "devDependencies": {
    "@trust/webcrypto": "^0.4.0",
    "@types/enzyme": "^2.8.6",
    "@types/enzyme-to-json": "^1.5.0",
    "@types/jest": "^20.0.7",
    "@types/js-yaml": "^3.9.1",
    "@types/marked": "^0.3.0",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/simple-mock": "0.0.27",
    "@types/text-encoding": "0.0.32",
    "@types/uuid": "^2.0.29",
    "@types/webmidi": "^2.0.1",
    "add-asset-html-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "0.23.1",
    "dotenv": "^2.0.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "firebase-tools": "^3.12.0",
    "gh-pages": "^0.12.0",
    "git-revision-webpack-plugin": "^2.4.1",
    "glob-all": "^3.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "jest-junit-reporter": "^1.1.0",
    "node-forge": "^0.7.1",
    "node-sass": "^3.13.1",
    "node-webcrypto-ossl": "^1.0.31",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.0",
    "sass-loader": "^3.1.2",
    "simple-mock": "^0.8.0",
    "style-loader": "0.13.0",
    "subtle": "^0.1.8",
    "text-encoding": "^0.6.4",
    "ts-jest": "^20.0.10",
    "ts-loader": "^2.3.3",
    "typescript": "^2.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "^4.1.0",
    "worker-loader": "^0.8.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js",
      "^builder(.*)$": "<rootDir>/src/lib/builder$1",
      "^config(.*)$": "<rootDir>/src/config$1",
      "^editor(.*)$": "<rootDir>/src/lib/editor$1",
      "^styled(.*)$": "<rootDir>/src/styled$1"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/tests/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(node_modules|app/build)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testResultsProcessor": "./node_modules/jest-junit-reporter"
  }
}
